<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javafx.windows.AcceptORDeclineController">
   <children>
      <BorderPane prefHeight="209.0" prefWidth="334.0">
         <bottom>
            <GridPane prefHeight="41.0" prefWidth="334.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="75.0" minWidth="10.0" prefWidth="24.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="119.0" minWidth="10.0" prefWidth="119.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="10.0" prefWidth="49.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="119.0" minWidth="10.0" prefWidth="119.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="62.0" minWidth="10.0" prefWidth="27.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="button" mnemonicParsing="false" onAction="#clickOkeyButton" prefHeight="34.0" prefWidth="116.0" text="ACCEPT" textAlignment="CENTER" GridPane.columnIndex="1">
                     <font>
                        <Font name="Arial Black" size="9.0" />
                     </font></Button>
                  <Button fx:id="button" mnemonicParsing="false" onAction="#clickDeclineButton" prefHeight="34.0" prefWidth="128.0" text="DECLINE" GridPane.columnIndex="3">
                     <font>
                        <Font name="Arial Black" size="9.0" />
                     </font></Button>
               </children>
            </GridPane>
         </bottom>
         <center>
            <Label prefHeight="86.0" prefWidth="336.0" text="Do you want to add your references to database?" textAlignment="CENTER" textFill="RED" wrapText="true" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
